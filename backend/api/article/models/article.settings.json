{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "meta_title": {
      "type": "string",
      "required": true
    },
    "meta_image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "published_at": {
      "type": "date",
      "required": true
    },
    "dynamic": {
      "type": "dynamiczone",
      "components": [
        "rows.page-header",
        "rows.stack"
      ]
    },
    "meta_description": {
      "type": "text",
      "required": true,
      "maxLength": 160
    },
    "title": {
      "type": "string",
      "private": true,
      "required": true,
      "unique": true
    }
  }
}
